<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="850.0" prefWidth="1000.0" stylesheets="@css/MyTickets.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="il.cshaifasweng.OCSFMediatorExample.client.controllers.AdminInbox">
    <children>
        <ScrollPane prefHeight="850.0" prefWidth="774.0" style="-fx-background-color: #bdc3c7;">
            <content>
                <VBox fx:id="MessageContainer" prefHeight="819.0" prefWidth="753.0" spacing="30.0" styleClass="main-pane" />
            </content>
        </ScrollPane>
      <Button fx:id="acceptBtn" layoutX="823.0" layoutY="81.0" mnemonicParsing="false" onAction="#onAccept" prefHeight="44.0" prefWidth="114.0" text="Accept">
         <font>
            <Font size="25.0" />
         </font>
      </Button>
      <Button fx:id="rejectBtn" layoutX="823.0" layoutY="148.0" mnemonicParsing="false" onAction="#onReject" prefHeight="44.0" prefWidth="114.0" text="Reject">
         <font>
            <Font size="25.0" />
         </font>
      </Button>
    </children>
</AnchorPane>
