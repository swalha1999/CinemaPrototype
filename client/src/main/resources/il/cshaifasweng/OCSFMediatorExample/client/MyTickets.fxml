    <children>
        <ScrollPane layoutX="14.0" layoutY="14.0" prefHeight="850.0" prefWidth="900">
            <content>
                <VBox fx:id="TicketsContainer" prefHeight="583.0" prefWidth="880.0" spacing="10.0" styleClass="main-pane"/>
