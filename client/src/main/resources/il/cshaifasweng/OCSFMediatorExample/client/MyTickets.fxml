<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="850.0" prefWidth="850.0" style="-fx-background-color: #bdc3c7;" stylesheets="@css/MyTickets.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="il.cshaifasweng.OCSFMediatorExample.client.controllers.MyTickets">
    <children>
        <ScrollPane layoutX="14.0" layoutY="14.0" prefHeight="678.0" prefWidth="900">
            <content>
                <VBox fx:id="TicketsContainer" prefHeight="656.0" prefWidth="878.0" spacing="10.0" styleClass="main-pane" />
            </content>
        </ScrollPane>

        <!-- Add the Return Ticket Button -->
        <Button fx:id="returnTicketButton" layoutX="364.0" layoutY="708.0" mnemonicParsing="false" onAction="#RemoveTicketBtn" styleClass="return-button" text="Return Ticket" />

    </children>
</AnchorPane>
